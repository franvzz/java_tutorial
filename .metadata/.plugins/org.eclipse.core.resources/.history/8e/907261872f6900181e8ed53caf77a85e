package paquete;

class Main {

	public static void main(String[] args) {
		// Algoritmos ciclicos
		// Serie fibonacci 1,1,2,3,5,8,11,...
		
		// Serie numeros impares
		for(int i=0; i<=10; i++) {
			// Si residuo es diferente de 0, es impar 
			if( (i % 2) != 0 ) {
				System.out.println("Número impar: " + i);
			}
		}
	}

}
