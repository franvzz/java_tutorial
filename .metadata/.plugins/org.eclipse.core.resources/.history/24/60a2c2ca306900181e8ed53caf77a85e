package paquete;

class Main {

	public static void main(String[] args) {
		// Switch y Case
		int x = 1;
		switch(x) {
			case 1: 
				System.out.println("X vale: 1");
				break;

			case 2: 
				System.out.println("X vale: 2");
				break;
			default:
				System.out.println("X vale: 0");
		}
		
	}

}
