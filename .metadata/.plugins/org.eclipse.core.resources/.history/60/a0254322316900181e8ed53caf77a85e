package paquete;

class Main {

	public static void main(String[] args) {
		// Switch y Case
		int x = 2;
		char y = 'c';
		switch(y) {
			case 1: 
				System.out.println("X vale: 1");
				break;
			case 2: 
				System.out.println("X vale: 2");
				break;
			default:
				System.out.println("Valor de x es C");
		}
		
	}

}
